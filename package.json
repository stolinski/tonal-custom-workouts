{
  "dependencies": {
    "@drop-in/theme": "workspace:^",
    "@drop-in/toast": "workspace:^",
    "pocketbase": "^0.21.3"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.2.2",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "pocketbase-typegen": "^1.2.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "svelte": "5.0.0-next.191",
    "svelte-check": "^3.8.4",
    "svelte-preprocess": "^6.0.2",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "vite": "^5.3.4"
  },
  "engines": {
    "node": "<20.11.1"
  },
  "name": "tonal",
  "scripts": {
    "build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "dev": "vite dev",
    "format": "prettier --write --plugin-search-dir=. .",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "make-package": "node ./src/packages/@drop-in/tools/package.js",
    "package": "svelte-kit package",
    "prepare": "svelte-kit sync",
    "preview": "svelte-kit preview",
    "types": "pocketbase-typegen --env --out ./src/types/pocketbase.ts"
  },
  "type": "module",
  "version": "0.0.4",
  "workspaces": [
    "src/packages/**"
  ]
}